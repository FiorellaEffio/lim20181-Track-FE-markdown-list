{
  "name": "mdlinks-effio",
  "version": "1.0.0",
  "description": "library to know if a file have url broken or not",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "htmlhint": "htmlhint src/index.html test/test.html",
    "eslint": "eslint --ext .js index.js src/app.js",
    "pretest": "npm run eslint && npm run htmlhint",
    "test-browser": "opener ./test/test.html",
    "test-node": "nyc jest ./test/headless.js && nyc report --reporter=html",
    "test": "npm run test-node && npm run test-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiorellaEffio/lim20181-Track-FE-markdown-list.git"
  },
  "dependencies": {
    "eslint": "^4.19.1",
    "htmlhint": "^0.9.13",
    "jest": "^23.5.0",
    "nyc": "^11.7.1",
    "opener": "^1.4.3"
  },
  "keywords": [
    "markdown",
    "links"
  ],
  "author": "Fiorella Effio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FiorellaEffio/lim20181-Track-FE-markdown-list/issues"
  },
  "homepage": "https://github.com/FiorellaEffio/lim20181-Track-FE-markdown-list#readme"
}
